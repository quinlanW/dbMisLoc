0px\">\n                <el-option v-for=\"item in options1\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\"></el-option>\n                </el-select>\n                <b>word_size:</b>\n                <el-input v-model=\"word_size\" placeholder=\"integer in [2,7]\" onkeyup=\"value=value.replace(/[^\\d]/g,'')\"></el-input>\n                <el-button class=\"sou\" type=\"success\" v-on:click=\"Search(evalue,word_size)\" style=\"margin-left: 10px; padding-left: 10px\" icon=\"el-icon-search\" plain>Blast</el-button>\n                <el-table :data=\"tableData\" v-show=\"show\" style=\"width: 100%; margin: 20px auto 0 auto\" >\n                  <!-- <el-table-column  label=\"Query sequence id\" prop=\"qseqid\" width=\"270\"></el-table-column> -->\n                  <el-table-column  label=\"Subject seq id\" prop=\"sseqid\" width=\"230\"></el-table-column>\n                  <el-table-column  label=\"Percentage(identical matches)\" prop=\"pident\" width=\"250\"></el-table-column>\n                  <el-table-column  label=\"Alignment length\" prop=\"length\" width=\"200\"></el-table-column>\n                  <el-table-column  label=\"Evalue\" prop=\"evalue\"></el-table-column>\n                  <el-table-column  label=\"Bitscore\" prop=\"bitscore\"></el-table-column>\n\n                  <el-table-column prop=\"\" label=\"Details\" type=\"expand\" width=\"90\">\n                    <template slot-scope=\"props\">\n                        <el-form label-position=\"left\" class=\"demo-table-expand\" label-width=\"auto\">\n                            <el-form-item label=\"Nucleotide sequences\">\n                                <span>{{ props.row.Nucleotide_Sequences_FASTA }}</span>\n                            </el-form-item>\n                        </el-form>\n                    </template>\n                </el-table-column>\n\n                </el-table>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport request from '@/utils/request'\n\nexport default {\n    data() {\n        return {\n            textarea: '',\n            show:false,\n            word_size:'2',\n            // 设置evalue下拉框选项\n            options1: [\n                {\n                value: '1e-4',\n                label: '1e-4'\n                }, {\n                value: '1e-3',\n                label: '1e-3'\n                },{\n                value: '1e-5',\n                label: '1e-5 '\n                },{\n                value: '1e-6',\n                label: '1e-6 '\n                },{\n                value: '1e-7',\n                label: '1e-7 '\n                },],\n            // 设置evalue默认值\n            evalue: '1e-4',\n            tableData:[]\n        }\n    },\n    methods: {\n        handleOpen(key, keyPath) {\n            console.log(key, keyPath);\n        },\n        handleClose(key, keyPath) {\n            console.log(key, keyPath);\n        },\n        evalue_change(val) {\n            this.evalue = val;\n        },\n        Search(evalue,word_size){\n            console.log(\"click serch\");\n            this.tableData = [];\n            this.show = true;\n            request({    \n                url: \"/api/proteins/getblastresult\",\n                method: \"POST\",\n                data:{\n                    seqquery:this.textarea,\n                    sizequery:evalue,\n                    wordsize:word_size\n                },\n            })\n            .then(res=>{\n                console.log(res)\n                // var result = {\n                //     'sequence_id': res.message.data[1],\n                //     'score': res.message.data[2],\n                //     'evalue': res.message.data[10]\n                // }\n                \n                for(let i=0;i<res.message.data.length;i++){\n                  let resdata = res.message.data[i];\n                  if(resdata!==\"\"){\n                    let qseqid = resdata.split('\\t')[0];\n                    let sseqid = resdata.split('\\t')[1];\n                    let pident = resdata.split('\\t')[2];\n                    let length = resdata.split('\\t')[3];\n                    let evalue = resdata.split('\\t')[4];\n                    let bitscore = resdata.split('\\t')[5];\n                    let result = {}\n                    // console.log(qseqid,sseqid,pident,length,evalue,bitscore);\n                    result = {\n                      \"qseqid\": qseqid,\n                      \"sseqid\": sseqid,\n                      \"pident\": pident,\n                      \"length\": length,\n                      \"evalue\": evalue,\n                      \"bitscore\": bitscore\n                    }\n                    console.log(result[\"qseqid\"]); //test\n                    this.tableData.push(result)\n                  }\n                }\n                // var temp = [result]\n                // this.tableData = temp\n                console.log(this.tableData);\n            })\n            .catch(err=>{\n                console.log(err);\n            })\n        }\n    }\n}\n</script>\n\n\n<style scoped>\n.el-select{\n  width: 150px !important;\n}\n.el-input{\n  width: 150px;\n}\n.el-select-dropdown__item{\n  text-align: center !important;\n}\n</style>\n<style scoped lang=\"less\">\n.myForm {\n  margin: 10px auto; /* 表单居中设置 */\n  padding: 40px 60px;\n  width: 70%;\n  border: 3px solid #b4ede7;\n  border-radius: 10px;\n}\n\n.el-table {\n  font-size: 15px;\n  //color: #232324;\n}\n\n/deep/ .el-form-item__label {\n  font-size: 15px;\n  //color: #232324;\n}\n\n.title {\n  text-align: center;\n  font-size: 1.5em;\n  line-height: 80px;\n  height: 80px;\n  background: #e6f0ef; /* Old browsers */\n  background: -moz-linear-gradient(\n    -45deg,\n    #e6f0ef 45%,\n    #b4ede7 100%\n  ); /* FF3.6-15 */\n  background: -webkit-linear-gradient(\n    -45deg,\n    #e6f0ef 45%,\n    #b4ede7 100%\n  ); /* Chrome10-25,Safari5.1-6 */\n  background: linear-gradient(\n