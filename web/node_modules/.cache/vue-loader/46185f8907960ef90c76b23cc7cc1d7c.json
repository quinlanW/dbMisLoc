  v-for=\"(item, index) in didna\"\n                :label=\"item\"\n                :prop=\"item\"\n                :key=\"index\"\n              >\n              </el-table-column>\n            </el-table>\n          </div>\n\n          <!-- Dinucleotide-RNA-Standard -->\n          <div>\n            <h3>\n              <a name=\"dinucleotiderna-standard\"></a\n              >Dinucleotide-RNA-Standardized\n            </h3>\n            <el-table\n              :data=\"dirnastandard\"\n              height=\"400\"\n              empty-text=\"cannot find\"\n              border\n              stripe\n              style=\"width: 100%\"\n            >\n              <el-table-column\n                prop=\"ID\"\n                label=\"ID\"\n                width=\"80\"\n                fixed\n              ></el-table-column>\n              <el-table-column\n                prop=\"PropertyName\"\n                label=\"PropertyName\"\n                width=\"200\"\n                fixed\n              ></el-table-column>\n              <el-table-column prop=\"ReferID\" label=\"ReferID\" width=\"100\">\n                <template slot-scope=\"scope\">\n                  <a\n                    v-if=\"scope.row.ReferID == 'REID0029'\"\n                    :href=\"reid0029\"\n                    target=\"_black\"\n                  >\n                    {{ scope.row.ReferID }}\n                  </a>\n                  <a\n                    v-else-if=\"scope.row.ReferID == 'REID0030'\"\n                    :href=\"reid0030\"\n                    target=\"_black\"\n                  >\n                    {{ scope.row.ReferID }}\n                  </a>\n                  <a\n                    v-else-if=\"\n                      scope.row.ReferID == 'REID0038' ||\n                        scope.row.ReferID == 'REID0044'\n                    \"\n                  >\n                    {{ scope.row.ReferID }}\n                  </a>\n                  <a v-else :href