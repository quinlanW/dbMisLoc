ID\" width=\"100\">\n                <template slot-scope=\"scope\">\n                  <a :href=\"url + scope.row.PubMedID\" target=\"_black\">\n                    {{ scope.row.PubMedID }}\n                  </a>\n                </template>\n              </el-table-column>\n              <el-table-column\n                v-for=\"(item, index) in dirna\"\n                :label=\"item\"\n                :prop=\"item\"\n                :key=\"index\"\n              >\n              </el-table-column>\n            </el-table>\n          </div>\n\n          <!-- Trinucleotide-DNA-Original -->\n          <div>\n            <h3><a name=\"trinucleotide\"></a>Trinucleotide-DNA-Original</h3>\n            <el-table\n              :data=\"tridnaoriginal\"\n              height=\"400\"\n              empty-text=\"cannot find\"\n              border\n              stripe\n              style=\"width: 100%\"\n            >\n              <el-table-column\n                prop=\"ID\"\n                label=\"ID\"\n                width=\"80\"\n                fixed\n              ></el-table-column>\n              <el-table-column\n                prop=\"PropertyName\"\n                label=\"PropertyName\"\n                width=\"200\"\n                fixed\n              ></el-table-column>\n              <el-table-column prop=\"ReferID\" label=\"ReferID\" width=\"100\">\n                <template slot-scope=\"scope\">\n                  <a\n                    v-if=\"scope.row.ReferID == 'REID0029'\"\n                    :href=\"reid0029\"\n                    target=\"_black\"\n                  >\n                    {{ scope.row.ReferID }}\n                  </a>\n                  <a\n                    v-else-if=\"scope.row.ReferID == 'REID0030'\"\n                    :href=\"reid0030\"\n                    target=\"_black\"\n                  >\n                    {{ scope.row.ReferID }}\n                  </a>\n                  <a\n                    v-else-if