CA;AACA,EAAA,OArHA,qBAqHA;AAAA;;AACA;AACA,QAAA,MAAA,GAAA;AACA,MAAA,OAAA,EAAA,GADA;AAEA,MAAA,IAAA,EAAA,CAFA;AAGA,MAAA,UAAA,EAAA,CAAA;AACA,QAAA,IAAA,EAAA,QADA;AAEA,QAAA,EAAA,EAAA,GAFA;AAGA,QAAA,GAAA,EAAA;AAHA,OAAA;AAHA,KAAA;AASA,IAAA,cAAA,CAAA,MAAA,CAAA,CAAA,IAAA,CACA,UAAA,GAAA,EAAA;AACA,MAAA,MAAA,CAAA,OAAA,GAAA,GAAA,CAAA,OAAA,CAAA,IAAA;AACA,UAAA,UAAA,GAAA,EAAA;AACA,UAAA,UAAA,GAAA,EAAA;AACA,UAAA,QAAA,GAAA,EAAA;;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,MAAA,CAAA,OAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,CAAA,IAAA,CAAA;AACA,mBAAA,MAAA,CAAA,OAAA,CAAA,CAAA,EAAA,MAAA,CAAA,SAAA;AADA,SAAA;;AAGA,QAAA,MAAA,CAAA,WAAA,CAAA,IAAA,CAAA;AACA,mBAAA,MAAA,CAAA,OAAA,CAAA,CAAA,EAAA,SAAA;AADA,SAAA,EAJA,CAOA;;;AACA,YAAA,OAAA,GAAA,MAAA,CAAA,OAAA,CAAA,CAAA,EAAA,4BAAA,EAAA,KAAA,CAAA,GAAA,EAAA,CAAA,EAAA,KAAA,CAAA,GAAA,CAAA;;AACA,aAAA,IAAA,EAAA,GAAA,CAAA,EAAA,EAAA,GAAA,OAAA,CAAA,MAAA,EAAA,EAAA,EAAA,EAAA;AACA,cAAA,CAAA,QAAA,CAAA,QAAA,CAAA,OAAA,CAAA,EAAA,CAAA,CAAA,IAAA,OAAA,CAAA,EAAA,CAAA,IAAA,MAAA,EAAA;AACA,YAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAAA,EAAA,CAAA;;AACA,YAAA,MAAA,CAAA,cAAA,CAAA,IAAA,CAAA;AACA,uBAAA,OAAA,CAAA,EAAA;AADA,aAAA;AAIA;AACA,SAjBA,CAkBA;;;AACA,YAAA,MAAA,CAAA,OAAA,CAAA,CAAA,EAAA,0BAAA,KAAA,MAAA,EAAA;AACA,UAAA,MAAA,CAAA,WAAA,CAAA,IAAA,CAAA;AACA,qBAAA,MAAA,CAAA,OAAA,CAAA,CAAA,EAAA,0BAAA;AADA,WAAA;AAGA;;AACA,YAAA,MAAA,CAAA,OAAA,CAAA,CAAA,EAAA,eAAA,KAAA,MAAA,EAAA;AACA,UAAA,MAAA,CAAA,WAAA,CAAA,IAAA,CAAA;AACA,qBAAA,MAAA,CAAA,OAAA,CAAA,CAAA,EAAA,eAAA;AADA,WAAA;AAGA,SA5BA,CA6BA;;;AACA,YAAA,SAAA,GAAA,MAAA,CAAA,OAAA,CAAA,CAAA,EAAA,qBAAA,EAAA,KAAA,CAAA,GAAA,CAAA;;AACA,aAAA,IAAA,GAAA,GAAA,CAAA,EAAA,GAAA,GAAA,SAAA,CAAA,MAAA,EAAA,GAAA,EAAA,EAAA;AACA,cAAA,MAAA,GAAA,SAAA,CAAA,GAAA,CAAA,CAAA,KAAA,CAAA,GAAA,EAAA,CAAA,CAAA;;AACA,cAAA,CAAA,UAAA,CAAA,QAAA,CAAA,MAAA,CAAA,IAAA,MAAA,IAAA,MAAA,EAAA;AACA,YAAA,UAAA,CAAA,IAAA,CAAA,MAAA;;AACA,YAAA,MAAA,CAAA,cAAA,CAAA,IAAA,CAAA;AACA,uBAAA;AADA,aAAA;AAGA;AACA;;AAEA,YAAA,SAAA,GAAA,MAAA,CAAA,OAAA,CAAA,CAAA,EAAA,iBAAA,EAAA,KAAA,CAAA,GAAA,CAAA;;AACA,aAAA,IAAA,GAAA,GAAA,CAAA,EAAA,GAAA,GAAA,SAAA,CAAA,MAAA,EAAA,GAAA,EAAA,EAAA;AACA,cAAA,MAAA,GAAA,SAAA,CAAA,GAAA,CAAA,CAAA,KAAA,CAAA,GAAA,EAAA,CAAA,CAAA;;AACA,cAAA,CAAA,UAAA,CAAA,QAAA,CAAA,MAAA,CAAA,IAAA,MAAA,IAAA,MAAA,EAAA;AACA,YAAA,UAAA,CAAA,IAAA,CAAA,MAAA;;AACA,YAAA,MAAA,CAAA,cAAA,CAAA,IAAA,CAAA;AACA,uBAAA;AADA,aAAA;AAGA;AACA;AACA;;AACA,MAAA,MAAA,CAAA,eAAA,GAAA,MAAA,CAAA,UAAA;AACA,KA3DA,EA4DA,UAAA,GAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,KA9DA;AAgEA,GAhMA;AAiMA,EAAA,KAAA,EAAA;AACA,IAAA,SADA,qBACA,GADA,EACA;AACA,UAAA,SAAA,GAAA,GAAA;;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,UAAA,SAAA,IAAA,EAAA,EAAA;AACA,QAAA,KAAA,CAAA,eAAA,GAAA,KAAA,CAAA,UAAA;AACA,OAFA,MAEA,IAAA,SAAA,IAAA,SAAA,EAAA;AACA,QAAA,KAAA,CAAA,eAAA,GAAA,KAAA,CAAA,WAAA;AACA,OAFA,MAEA,IAAA,SAAA,IAAA,4BAAA,EAAA;AACA,QAAA,KAAA,CAAA,eAAA,GAAA,KAAA,CAAA,cAAA;AACA,OAFA,MAEA,IAAA,SAAA,IAAA,0BAAA,EAAA;AACA,QAAA,KAAA,CAAA,eAAA,GAAA,KAAA,CAAA,WAAA;AACA,OAFA,MAEA,IAAA,SAAA,IAAA,eAAA,EAAA;AACA,QAAA,KAAA,CAAA,eAAA,GAAA,KAAA,CAAA,WAAA;AACA,OAFA,MAEA,IAAA,SAAA,IAAA,qBAAA,EAAA;AACA,QAAA,KAAA,CAAA,eAAA,GAAA,KAAA,CAAA,cAAA;AACA,OAFA,MAEA,IAAA,SAAA,IAAA,iBAAA,EAAA;AACA,QAAA,KAAA,CAAA,eAAA,GAAA,KAAA,CAAA,cAAA;AACA;AACA;AAnBA;AAjMA,CAAA,C,CAuNA;AACA;AACA;AACA;AACA;AACA","sourcesContent":["<template>\n<!-- 模糊查询 miscondition 不查询图片缩放适应问题 未解决  -->\n    <div class=\"result\">\n        <div class=\"header\">\n        <!--element-ui的复合型输入框 可通过 slot 来指定在 input 中前置或者后置内容。 -->\n        <el-row class=\"title\" style=\"color: #606060; text-align: center; font-size: 1.5em; line-height: 80px; height: 80px;\">\n          <img src=\"../assets/img/search_label.png\" style=\"height: 45px; width: auto;  vertical-align: middle; margin-right: 10px; margin-bottom: 5px;\"/>\n            Search from Database\n        </el-row>\n        <!-- 输入后匹配输入建议会出现渲染不及时的错误，改为输入前匹配输入建议 -->\n      \n        <el-autocomplete\n            placeholder=\"Enter the query\"\n            v-model=\"inputContent\"\n            clearable\n            @keyup.enter.native=\"Search\"\n            class=\"input-with-select\"\n            :fetch-suggestions=\"querySearch\"\n        >\n            <el-select\n            v-model=\"searchOpt\"\n            slot=\"prepend\"\n            clearable\n            placeholder=\"Search Options\"\n            class=\"select\"\n            >\n                <el-option-group\n                    v-for=\"group in options\"\n                    :key=\"group.label\"\n                    :label=\"group.label\"\n                >\n                    <el-option\n                    v-for=\"item in group.options\"\n                    :key=\"item.value\"\n                    :label=\"item.label\"\n                    :value=\"item.value\"\n                    >\n                    </el-option>\n                </el-option-group>\n            </el-select>\n            <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"Search\"></el-button>\n        </el-autocomplete>\n        </div>\n\n        <!-- <div>\n          <p>{{ propertyresults }}</p>\n        </div> -->\n\n        <div class=\"content\" style=\"height: 70vh; overflow-x: hidden\">\n          <!-- 未进行查询 没有数据显示 -->\n          <el-row v-if=\"id==0\" type=\"flex\" justify=\"space-between\" style=\"padding: 5px;\" class=\"content\">\n            <el-col :span=\"12\">\n              <img src=\"../assets/img/search_ndata.png\" alt=\"\" style=\"height: auto; width:100%;\">\n            </el-col>\n            <el-col :span=\"12\" class=\"explain\">\n              <p>\n                You can obtain the desired information by selecting the search type and searching \n                for the corresponding name. If you forget how to spell it, a fuzzy search can be \n                performed by “_\" or \"%\". For example, you want the \"PEX19\", which can be obtained \n                by searching “PE_19\".More detailed fuzzy query rules are given in 'Search help' in \n                the tutorial section of the 'Help' page.\n\n              </p>\n              <!-- <img src=\"\" alt=\"\"> -->\n              <img\n                alt=\"search example\"\n                style=\"height: auto; width: 100%;\"\n                src=\"../assets/img/search_example.png\"\n              />\n            </el-col>\n          </el-row>\n          <!-- 展示表格  -->\n          <div class=\"tabletitle\" v-if=\"id==1\">\n            <el-row>\n              <el-col :span=\"4\" :offset=\"10\">\n                Search results\n              </el-col>\n            </el-row>\n          </div>\n          <el-table\n            v-if=\"id==1\"\n            id=\"myTable1\"\n            :header-cell-style=\"{background:'#eef1f6',color:'#606266'}\"\n            :data=\"proteinSchTable\"\n            :row-class-name=\"tabRowClassName\"\n            empty-text=\"No data available\"\n            border\n            style=\"width: 100%\"\n            ref=\"table\"\n          >\n            <el-table-column prop=\"Protein\" label=\"Protein\"></el-table-column>\n            <el-table-column prop=\"Normal_localization\" label=\"Normal localization\"></el-table-column>\n            <el-table-column prop=\"Mislocalization\" label=\"Mislocalization\" ></el-table-column>\n            <el-table-column prop=\"Data_sources\" label=\"PubMed ID\">\n              <template slot-scope=\"value\">\n                <a  v-for=\"(goid,index) in value.row.Data_sources.replace(/\\s*/g,'').split(',')\" :key=\"index\" :href=\"'https://pubmed.ncbi.nlm.nih.gov/'+goid.slice(5,13)\"\n                    target=\"_blank\" class=\"TestCSS\">{{ goid }}\n                  <br></a>\n              </template>\n            </el-table-column>\n            <el-table-column prop=\"Uniprot_Entry\" label=\"Uniprot ID\" >\n              <template slot-scope=\"value\">\n                <a :href=\"'https://www.uniprot.org/uniprot/'+value.row.Uniprot_Entry\" target=\"_blank\" class=\"TestCSS\">\n                  {{value.row.Uniprot_Entry}}\n                </a>\n              </template>\n            </el-table-column>\n            <el-table-column prop=\"Uniprot_Accession_number\" label=\"Uniprot AC\" ></el-table-column>\n\n            <el-table-column prop=\"\" label=\"Details\" type=\"expand\" width=\"100\">\n              <template slot-scope=\"props\">\n                <el-form label-position=\"left\" class=\"demo-table-expand\" label-width=\"auto\" style=\"font-family: monospace;\">\n                  <!-- <el-form-item label=\"Uniprot AC\">\n                    <span>{{ props.row.Uniprot_Accession_number }}</span>\n                  </el-form-item> -->\n                  <el-form-item label=\"Normal localization GO\">\n                    <!-- <a  v-for=\"(goid,index) in props.row.Normal_localization_GO_ID.split(',')\" :key=\"index\" :href=\"'https://www.ebi.ac.uk/QuickGO/term/'+goid.split('[')[1].split(']')[0]\"\n                        target=\"_blank\" class=\"TestCSS\">{{ goid }}\n                    <br></a> -->\n                    <span>{{ props.row.Normal_localization_GO_ID }}</span>\n                  </el-form-item>\n                  <el-form-item label=\"Mislocalization GO\">\n                    <!-- <a  v-for=\"(goid,index) in props.row.Mislocalization_GO_ID.split(',')\" :key=\"index\" :href=\"'https://www.ebi.ac.uk/QuickGO/term/'+goid.split('[')[1].split(']')[0]\"\n                        target=\"_blank\" class=\"TestCSS\">{{ goid }}\n                    <br></a> -->\n                    <span>{{ props.row.Mislocalization_GO_ID }}</span>\n                  </el-form-item>\n                  <el-form-item label=\"Protein sequence\">\n                    <span>{{ props.row.Nucleotide_Sequences_FASTA }}</span>\n                  </el-form-item>\n                  <el-form-item label=\"Mislocalization condition\">\n                    <span>{{ props.row.Mislocalization_conditions }}</span>\n                  </el-form-item>\n                </el-form>\n              </template>\n            </el-table-column>\n          </el-table>\n          <template>\n            <el-backtop target=\".content\" :visibility-height=\"100\" :bottom=\"40\">\n              <div\n                style=\"{\n                  height: 100%;\n                  width: 100%;\n                  background-color: #151D3B;\n                  box-shadow: 0 0 6px rgba(0,0,0, .12);\n                  text-align: center;\n                  line-height: 40px;\n                  color: #EFFFFD;\n                }\"\n              >\n                <img \n                  style=\"height: 20px; width: auto; vertical-align: middle;\"\n                  src=\"../assets/img/backtotop.png\" alt=\"\"\n                >\n              </div>\n            </el-backtop>\n          </template>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { getProteinData } from \"@/api/dataReq.js\"\n\nexport default {\n    data() {\n\t\treturn{\n\t\t\tproteinSchTable: [], // 搜索展示数据\n\t\t\tproteinMorTable: [],\n\t\t\trecData: [], // 接受数据\n\t\t\tinputContent: \"\", // 输入查询数据\n\t\t\tsearchOpt: \"Protein\", // 选择查询类型\n\t\t\toptions: [\n\t\t\t\t{\n\t\t\t\t\tlabel: \"Protein Related\",\n\t\t\t\t\toptions: [\n\t\t\t\t\t\t{ value: \"Protein\", label: \"Protein Name\"},\n\t\t\t\t\t\t// { value: \"Mislocalization_condition\", label: \"Mis-condition\"}\n\t\t\t\t\t]\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tlabel: \"Uniprot Related\",\n\t\t\t\t\toptions: [\n\t\t\t\t\t\t{ value: \"Uniprot_Accession_number\", label: \"Uniprot AC\"},\n\t\t\t\t\t\t{ value: \"Uniprot_Entry\", label: \"Uniprot ID\"}\n\t\t\t\t\t]\n\t\t\t\t},\n        // 20220309\n        {\n          label: \"Localization Related\",\n          options: [\n            { value: \"Normal_localization\", label: \"Normal Localization\"},\n            { value: \"Mislocalization\", label: \"Mis-Localization\"},\n          ]\n        },\n        {\n          label: \"Condition Related\",\n          options: [\n            { value: \"Mislocalization_conditions\", label: \"Mis-Condition\"}\n          ]\n        }\n\t\t\t],\n\t\t\tid: 0,  // 查询标记\n\t\t\tactiveNames: [],\n\t\t\t// 用于模糊匹配的输入建议\n\t\t\tproperties: [],\n\t\t\tpropertyresults: [],\n\t\t\tproproperty: [],\n\t\t\tmisconproperty: [],\n\t\t\tuacproperty: [],\n\t\t\tuidproperty: [],\n      norlocproperty: [],\n      mislocproperty: [],\n\t\t}\n    },\n    methods: {\n        // tabRowClassName({ row, rowIndex }) {\n        tabRowClassName({ rowIndex }) {\n          // console.log(row);\n          var index = rowIndex + 1;\n          if (index % 2 == 0) {\n            return \"warning-row\";\n          }\n          // console.log(rowIndex);\n        },\n        querySearch(queryString, cb) {\n          var properties = this.propertyresults;\n          // console.log(\"q\", properties);\n          var results = queryString\n            ? properties.filter(this.createFilter(queryString))\n            : properties;\n          cb(results);\n        },\n        // !==-1 表示可以匹配包含的，而不是首字母\n        createFilter(queryString) {\n          return property => {\n            return (\n              // 这里必须是property.value 才有效?\n              property.value.toLowerCase().indexOf(queryString.toLowerCase()) === 0\n            );\n          };\n        },\n        Search() {\n            // // 将输入的理化性质大写均转为小写(但是sql好像不区分大小写)\n            // var inputContent = this.inputContent.toLowerCase();\n            // // console.log(propertyName);\n            // inputContent = sqlencode(inputContent);\n            // console.log(\"click search\");\n            var searchOpt = this.searchOpt;\n            // var _this = this;\n            let param ={\n              \"perPage\":200,\n              \"page\":1,\n              \"conditions\": [{\n                \"prop\": searchOpt,\n                \"op\": \"like\",\n                \"val\": '%'+this.inputContent+'%',\n              }]\n            };\n            getProteinData(param).then(\n              res=>{\n                // console.log(\"res: \"+res)\n                this.proteinSchTable = res.message.info;\n                // console.log(this.proteinSchTable)\n              },\n              err=>{\n                console.log(err)\n              }\n            )\n            this.id = 1;\n        },\n    },\n\n    // filterProperty(){\n        //   for(let i=0;i<this.uacproperty.length;i++){\n        //     if(this.uacproperty[i][\"value\"]=='n.a.'){\n        //       console.log(this.uacproperty[i]);\n        //       delete this.uacproperty[i];\n        //     }\n        //   }\n        // },\n    mounted() {\n      // var _this = this;\n      let params = {\n        perPage: 200,\n        page: 1,\n        conditions: [{\n          prop: \"num_id\",\n          op: \">\",\n          val: 1\n        }]\n      };\n      getProteinData(params).then(\n        res=>{\n          this.recData = res.message.info;\n          let norlocflag = [];\n          let mislocflag = [];\n          let condflag = [];\n          for(var i=0;i<this.recData.length;i++){\n            this.properties.push({\n              \"value\": this.recData[i][this.searchOpt]\n            });\n            this.proproperty.push({\n              \"value\": this.recData[i][\"Protein\"]\n            });\n            // Condition\n            let condStr = this.recData[i][\"Mislocalization_conditions\"].split('[')[0].split(',');\n            for(let i=0;i<condStr.length;i++){\n              if(!condflag.includes(condStr[i])&&condStr[i]!='n.a.'){\n                condflag.push(condStr[i]);\n                this.misconproperty.push({\n                  \"value\": condStr[i]\n                });\n\n              }\n            }\n            // Uniprot\n            if(this.recData[i][\"Uniprot_Accession_number\"] != 'n.a.'){\n              this.uacproperty.push({\n                \"value\": this.recData[i][\"Uniprot_Accession_number\"]\n              });\n            }\n            if(this.recData[i][\"Uniprot_Entry\"] != 'n.a.'){\n              this.uidproperty.push({\n                \"value\": this.recData[i][\"Uniprot_Entry\"]\n              });\n            }\n            // localization\n            let norlocStr = this.recData[i][\"Normal_localization\"].split(',');\n            for(let i=0;i<norlocStr.length;i++){\n              let norloc = norlocStr[i].split('[')[0];\n              if(!norlocflag.includes(norloc)&&norloc!='n.a.'){\n                norlocflag.push(norloc);\n                this.norlocproperty.push({\n                  \"value\": norloc\n                });\n              }\n            }\n\n            let mislocStr = this.recData[i][\"Mislocalization\"].split(',');\n            for(let i=0;i<mislocStr.length;i++){\n              let misloc = mislocStr[i].split('[')[0];\n              if(!mislocflag.includes(misloc)&&misloc!='n.a.'){\n                mislocflag.push(misloc);\n                this.mislocproperty.push({\n                  \"value\": misloc\n                });\n              }\n            }\n          }\n          this.propertyresults = this.properties;\n        },\n        err=>{\n          console.log(err);\n        }\n      );\n    },\n    watch:{\n      searchOpt(val){\n        var searchOpt = val;\n        var _this = this;\n        if (searchOpt == \"\") {\n          _this.propertyresults = _this.properties;\n        } else if (searchOpt == \"Protein\") {\n          _this.propertyresults = _this.proproperty;\n        } else if (searchOpt == \"Mislocalization_conditions\") {\n          _this.propertyresults = _this.misconproperty;\n        } else if(searchOpt == \"Uniprot_Accession_number\") {\n          _this.propertyresults = _this.uacproperty;\n        } else if(searchOpt == \"Uniprot_Entry\") {\n          _this.propertyresults = _this.uidproperty;\n        } else if(searchOpt == \"Normal_localization\") {\n          _this.propertyresults = _this.norlocproperty;\n        } else if(searchOpt == \"Mislocalization\") {\n          _this.propertyresults = _this.mislocproperty;\n        } \n      }\n    }\n};\n// 模糊查询 后端实现\n// var sqlencode = function(str) {\n//   var sql = str.replace(/_/g, \"\\\\_\");\n//   sql = sql.replace(/\\*/g, \"%\"); // 用户可以输入带*的模糊查询\n//   return sql;\n// };\n</script>\n\n<style>\n/* 没有用scoped 如果使用斑马线设置无效 */\n.header {\n  width: 100%;\n  padding-bottom: 60px;\n  background: #e6f0ef; /* Old browsers */\n  background: -moz-linear-gradient(\n    200deg,\n    #9AD0EC 60%,\n    #398AB9 80%\n  ); /* FF3.6-15 */\n  background: -webkit-linear-gradient(\n    200deg,\n    #9AD0EC 60%,\n    #398AB9 80%\n  ); /* Chrome10-25,Safari5.1-6 */\n  background: linear-gradient(\n    200deg,\n    #9AD0EC 60%,\n    #398AB9 80%\n  ); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */\n}\n.input-with-select {\n  width: 60%;\n  /* font-size: 15px; */\n}\n.el-collapse-item__header {\n  font-size: 16px;\n}\n\n.el-select {\n  width: 200px;\n}\n\n.el-select-group__title {\n  text-align: left;\n}\n\n.el-select-dropdown__item {\n  text-align: left;\n  text-indent: 2em;\n}\n\n.el-input {\n  font-size: 15px;\n}\n\n.content {\n  padding: 10px;\n}\n\n.el-table {\n  font-size: 15px;\n  color: #232324;\n  white-space: pre-line;\n}\n\n.el-table .warning-row {\n  background: #9AD0EC;\n}\n.tabletitle {\n  text-align: center;\n  background-color: #9AD0EC;\n  height: 30px;\n  line-height: 30px;\n  color: #686868;\n  font-weight: bold;\n}\n.explain {\n  border: solid 1px #398AB9;\n  padding: 10px;\n}\n.explain p {\n  text-indent: 2em;\n  text-align: justify;\n}\n\n.demo-table-expand {\n    font-size: 0;\n}\n.demo-table-expand label {\n  width: 90px;\n  color: #99a9bf;\n}\n.demo-table-expand {\n  margin-right: 0;\n  margin-bottom: 0;\n  width: 100%;\n}\n.el-form-item {\n  float: left;\n  clear: both;\n  margin-right: 0;\n  margin-bottom: 0;\n  width: 100%;\n}\n\n</style>\n<style lang=\"scss\" scoped>\n// /deep/会报错 用::v-deep\n::v-deep .el-form-item__label {\n  float: none; // 取消label左浮动\n  word-break: break-word; // 支持单词截断换行\n}\n::v-deep .el-table__expand-icon{\n -webkit-transform: rotate(0deg);\n transform: rotate(0deg);\n}\n</style>"],"sourceRoot":"src/views"}]}